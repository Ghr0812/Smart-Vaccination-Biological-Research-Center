     <property name="styleSheet">
      <string notr="true">/* Agrandir les zones de texte QLineEdit */
QLineEdit {
    min-height: 35px; /* Hauteur minimale */
    font-size: 14pt;  /* Taille de texte */
    padding: 8px;     /* Espacement interne */
    border: 1px solid #8c8c8c;
    border-radius: 5px;
    background-color: #ffffff;
}

/* Surbrillance des QLineEdit quand ils sont sélectionnés */
QLineEdit:focus {
    border-color: #0078d7; /* Bordure bleue en surbrillance */
    outline: none;
}

/* Agrandir les éditeurs de date QDateEdit */
QDateEdit {
    min-height: 35px; /* Hauteur minimale */
    font-size: 14pt;  /* Taille de texte */
    padding: 8px;     /* Espacement interne */
    border: 1px solid #8c8c8c;
    border-radius: 5px;
    background-color: #ffffff;
}

/* Surbrillance des QDateEdit quand ils sont sélectionnés */
QDateEdit:focus {
    border-color: #0078d7;
    outline: none;
}

/* Agrandir les boutons radio QRadioButton */
QRadioButton {
    font-size: 14pt;  /* Taille du texte */
    padding: 5px;     /* Espacement autour */
}

/* Style des indicateurs des QRadioButton */
QRadioButton::indicator {
    width: 20px;   /* Largeur de l'indicateur */
    height: 20px;  /* Hauteur de l'indicateur */
    border: 2px solid #8c8c8c;
    border-radius: 10px;
    background-color: #ffffff;
}

QRadioButton::indicator:checked {
    background-color: #0078d7; /* Couleur quand sélectionné */
    border: 2px solid #0056a4;
}

/* Agrandir les listes déroulantes QComboBox */
QComboBox {
    min-height: 35px;
    font-size: 14pt;
    padding: 5px;
    border: 1px solid #8c8c8c;
    border-radius: 5px;
    background-color: #ffffff;
}

/* Surbrillance des QComboBox quand elles sont sélectionnées */
QComboBox:focus {
    border-color: #0078d7;
    outline: none;
}

/* Style des éléments déroulants dans QComboBox */
QComboBox QAbstractItemView {
    font-size: 14pt;
    border: 1px solid #8c8c8c;
    background-color: #ffffff;
    selection-background-color: #d3d3d3; /* Couleur de sélection */
}



/* Style du QGroupBox avec un thème de vaccination et de biologie */
QGroupBox {
    border: 2px solid #006d5b; /* Bordure vert foncé (santé et nature) */
    border-radius: 10px;
    margin-top: 20px; /* Espacement pour le titre */
    padding: 10px;    /* Espacement interne */
    background: qlineargradient(
        x1: 0, y1: 0, x2: 1, y2: 1,
        stop: 0 #7bdcb5, /* Vert clair évoquant la nature */
        stop: 0.5 #98c1d9, /* Bleu doux pour un aspect clinique */
        stop: 1 #eef4f7 /* Blanc légèrement teinté pour la neutralité */
    );
    color: #004c3f; /* Texte vert foncé pour un contraste agréable */
    font-size: 14pt; /* Taille de texte augmentée */
}

/* Style du titre du QGroupBox */
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center; /* Centrer le titre */
    padding: 5px; /* Espacement autour du texte */
    background-color: #eef4f7; /* Fond blanc teinté */
    color: #006d5b; /* Couleur du texte assortie à la bordure */
    font-size: 16pt;
    font-weight: bold; /* Mettre le titre en gras */
    border-radius: 5px;
}</string>
