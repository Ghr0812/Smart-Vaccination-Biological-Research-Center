   <property name="styleSheet">
    <string notr="true">/* Style global */
QWidget {
    background-color: #1B8A6B  ; /* Bleu sérum */
    color: #ffffff;
    font-family: &quot;Arial&quot;, sans-serif;
    font-size: 14pt;
	border-radius: 35px; /* Bordure arrondie comme l’image */
}

/* Champs de saisie QLineEdit */
QLineEdit {
    min-height: 40px;
    font-size: 15pt;
    padding: 10px;
    border: 2px solid #0096C7;
    border-radius: 25px; /* Bordure arrondie comme l’image */
    background-color: #ffffff;
    color: #333333;
}

QLineEdit:focus {
    border-color: #D4A017;
    box-shadow: 0px 0px 8px #D4A017;
}

/* Éditeurs de date QDateEdit */
QDateEdit {
    min-height: 40px;
    font-size: 15pt;
    padding: 10px;
    border: 2px solid #0096C7;
    border-radius: 25px; /* Bordure arrondie */
    background-color: #ffffff;
    color: #333333;
}

QDateEdit:focus {
    border-color: #D4A017;
    box-shadow: 0px 0px 8px #D4A017;
}

/* Listes déroulantes QComboBox */
QComboBox {
    min-height: 40px;
    font-size: 15pt;
    padding: 8px;
    border: 2px solid #0096C7;
    border-radius: 25px; /* Bordure arrondie */
    background-color: #ffffff;
    color: #333333;
}

QComboBox:focus {
    border-color: #D4A017;
    box-shadow: 0px 0px 8px #D4A017;
}

/* Style des éléments déroulants */
QComboBox QAbstractItemView {
    font-size: 14pt;
    border: 1px solid #0096C7;
    selection-background-color: #D4A017;
}

/* Boutons avec bords arrondis */
QPushButton {
    min-height: 45px;
    font-size: 16pt;
    font-weight: bold;
    border-radius: 25px; /* Bordure arrondie */
    background-color: #5FAF6D;
    color: white;
    border: 2px solid #D4A017;
    transition: all 0.3s ease;
}

QPushButton:hover {
    background-color: #D4A017;
    color: #ffffff;
    border: 2px solid #0096C7;
}

QPushButton:pressed {
    background-color: #5FAF6D;
    color: white;
    border: 2px solid #ffffff;
}
</string>
